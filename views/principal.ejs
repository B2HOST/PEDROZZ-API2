<script>
function mostrarCalendario() {
var calendario = document.getElementById("calendario");
calendario.classList.toggle("show");
}

function atualizarHorario() {
var agora = new Date();
var hora = agora.getHours();
var minuto = agora.getMinutes();
var segundo = agora.getSeconds();

hora = hora < 10 ? "0" + hora : hora;
minuto = minuto < 10 ? "0" + minuto : minuto;
segundo = segundo < 10 ? "0" + segundo : segundo;

var horario = hora + ":" + minuto + ":" + segundo;
document.getElementById("horario").innerHTML = horario;

var diaSemana = agora.toLocaleDateString("pt-BR", { weekday: "long" });
var dia = agora.getDate();
var mes = agora.toLocaleDateString("pt-BR", { month: "long" });
var ano = agora.getFullYear();

var data = diaSemana + ", " + dia + " de " + mes + " de " + ano;
document.getElementById("data").innerHTML = dia;
document.getElementById("diaSemana").innerHTML = diaSemana;
document.getElementById("mes").innerHTML = mes;
document.getElementById("ano").innerHTML = ano;
}

atualizarHorario();
setInterval(atualizarHorario, 1000);
</script>


